{
  "name": "sokoban-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "gen:levels": "tsc -p tsconfig.scripts.json && node dist-scripts/scripts/gen-levels.js",
    "format": "prettier --check 'src/**/*.{js,jsx,ts}' 'scripts/**/*.{js,jsx,ts}'",
    "format:fix": "prettier --write 'src/**/*.{js,jsx,ts}' 'scripts/**/*.{js,jsx,ts}'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^4.0.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^6.0.7",
    "vitest": "^4.0.18"
  }
}
