{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist-scripts",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "target": "ES2022",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["scripts/**/*.ts", "src/**/*.ts"]
}
